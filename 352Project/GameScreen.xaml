<Window x:Class="_352Project.GameScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_352Project"
        mc:Ignorable="d"
        Title="GameScreen" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FF1E194E" Offset="0.029"/>
                <GradientStop Color="#FF335EA4" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Image x:Name="llama" Margin="87,111,591,201.667" Source="llama.png" Stretch="Fill"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="10,389,0,5" TextWrapping="Wrap" Text="Time: " VerticalAlignment="Top" Width="42" BorderBrush="{x:Null}"/>
        <TextBox x:Name="time" HorizontalAlignment="Left" Height="23" Margin="52,389,0,5" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="40" BorderBrush="{x:Null}" RenderTransformOrigin="0.025,0.572"/>


    </Grid>
</Window>
